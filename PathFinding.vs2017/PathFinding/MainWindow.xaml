<Window x:Class="PathFinding.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PathFinding"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Window.Resources>
        <local:AlgoViewModel x:Key="algoviewmodel"/>
    </Window.Resources>
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <TextBox Width="80" Text="{Binding Source={StaticResource algoviewmodel}, XPath=PopCount, Mode=TwoWay}" />
            </StackPanel>
            <Button Command="{Binding ClickCommand}" Width="70" Height="70" Content="LAUNCH"/>
            <WrapPanel Width="500">
                
            </WrapPanel>
        </StackPanel>
    </Grid>
</Window>
